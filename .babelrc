{
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "syntax-dynamic-import",
    "transform-class-properties",
    "styled-components",
    "transform-async-to-generator",
    "@babel/plugin-transform-runtime"
  ],
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": [
          "last 1 version",
          "ie >= 11",
          "safari >= 9",
          "ios >= 9",
          "android >= 4.4"
        ]
      }
    }],
    "@babel/preset-react"
  ],
  "compact": false
}
