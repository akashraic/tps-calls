  },
  "scripts": {
    "start": "NODE_ENV=development && node scripts/start.js",
    "start:lambda": "NODE_ENV=development netlify-lambda -c ./config/webpackFunctions.config.js serve src/js/lambda",
    "build": "node scripts/build.js",
    "build:lambda": "NODE_ENV=production netlify-lambda -c ./config/webpackFunctions.config.js build src/js/lambda",
    "netlify-deploy": "yarn version && yarn build && yarn build:lambda && netlify deploy --dir build",
    "netlify-deploy:prod": "netlify deploy --prod",
    "test": "node scripts/test.js --updateSnapshot"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/js/tests/*.js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
