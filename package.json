{
  "name": "tps-calls",
  "version": "1.5.1",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development && webpack-dev-server --hot",
    "build": "NODE_ENV=production && webpack --mode production --progress",
    "netlify-deploy": "yarn build && netlify deploy --dir dist",
    "netlify-deploy:prod": "netlify deploy --prod",
    "test": "node scripts/test.js --updateSnapshot"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "d3-ease": "^1.0.5",
    "date-fns": "^1.30.1",
    "firebase": "^6.0.4",
    "prettier": "^1.14.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "react-map-gl": "^4.1.3",
    "react-mapbox-gl": "^4.6.0",
    "react-pose": "^4.0.8",
    "react-redux": "^7.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-visibility-sensor": "^5.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.1",
    "tps-calls-shared": "git+ssh://git@github.com:rdrnt/tps-calls-shared.git",
    "tslib": "^1.9.3",
    "typeface-raleway": "^0.0.54",
    "typeface-roboto": "^0.0.75",
    "use-debounce": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/d3-ease": "^1.0.8",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-icons": "^3.0.0",
    "@types/react-map-gl": "^4.1.2",
    "@types/react-redux": "^7.1.1",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^4.1.16",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.4",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "html-webpack-plugin": "^3.0.7",
    "json-loader": "^0.5.7",
    "react-hot-ts": "^0.7.3",
    "redux-devtools-extension": "^2.13.8",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.1",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "3.2.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.2",
    "webpack-pwa-manifest": "^4.0.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/js/tests/*.js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  }
}
